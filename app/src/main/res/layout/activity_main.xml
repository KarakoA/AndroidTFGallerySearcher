<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/margin_activity_bottom"
    android:layout_marginEnd="@dimen/margin_activity_right"
    android:layout_marginStart="@dimen/margin_activity_left"
    android:layout_marginTop="@dimen/margin_activity_top"
    tools:context="de.htw_berlin.f4.ml.gallerysearcher.activities.MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/gallery_button_text"
        android:onClick="onNavigateToGalleryClicked"/>

</LinearLayout>
